  "dependencies": {
    "concurrently": "^7.5.0",
    "cool-ascii-faces": "^1.3.4",
    "nodemon": "^2.0.20"
  },
  "engines": {
    "node": "18.x"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  }
